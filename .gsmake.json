{
    "name":"github.com/gschat/gschat",

    "domain":"golang|gslang",

    "import":[
        {"name" : "github.com/gsmake/golang","domain":"task","version":"release/v3.0"},
        {"name" : "github.com/gsrpc/gsrpc","domain":"task|gslang","version":"release/v1.0"},
        {"name" : "github.com/gsrpc/gorpc","version":"release/v2.0"},
	    {"name" : "github.com/gsdocker/gsproxy","version":"release/v2.0"}
    ],

    "properties":{
        "gsrpc":{
            "golang":["github.com/gsrpc/gsrpc","./gschat.gs","./service.gs"]
        },

        "gsmake.golang.test" :[
           {"dir":"./gw","flags":"-bench . -cpu 4 "},
           {"dir":"./im","flags":"-bench . -cpu 4 "}
       ],

       "gsmake.golang.binary" :{
             "simulator" : "cmd/simulator",
             "gschat-proxy" : "cmd/proxy",
             "gschat-server" : "cmd/server"
       }
    }
}
